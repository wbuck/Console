                </Grid>
            </Viewbox>
        </VisualBrush.Visual>
    </VisualBrush>
    
    <VisualBrush x:Key="ExclamationVisualBrush">
        <VisualBrush.Visual>
            <Grid>
                <Rectangle Fill="{StaticResource HighlightSolidColorBrush}"
                           Height="30" 
                           Width="11" 
                           StrokeThickness="0.8" 
                           VerticalAlignment="Bottom" 
                           Margin="0,0,0,12"/>
                <Rectangle Fill="{StaticResource HighlightSolidColorBrush}" 
                           Height="8" 
                           Width="11" 
                           StrokeThickness="0.8" 
                           VerticalAlignment="Bottom"/>
            </Grid>
        </VisualBrush.Visual>
    </VisualBrush>

